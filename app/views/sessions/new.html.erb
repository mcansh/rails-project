<h1>Login!</h1>
<%= form_tag(sessions_path) do %>
  <fieldset>
    <%= label_tag :email %>
    <%= email_field_tag :email, '', placeholder: 'first@last.com'  %>
  </fieldset>
  <fieldset>
    <%= label_tag :password %>
    <%= password_field_tag :password, '', placeholder: 'password'  %>
  </fieldset>
  <fieldset>
    <%= submit_tag %>
  </fieldset>
<% end %>
