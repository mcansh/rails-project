<%= form_tag(sessions_path) do %>
  <h1>Sign In</h1>
  <fieldset>
    <%= label_tag :email, 'Email or Username' %>
    <%= text_field_tag :email %>
  </fieldset>
  <fieldset>
    <%= label_tag :password %>
    <%= password_field_tag %>
  </fieldset>
  <fieldset>
    <%= button_tag 'Sign In' %>
  </fieldset>

<% end %>
<%= link_to "Sign in with GitHub", '/auth/github' %>
